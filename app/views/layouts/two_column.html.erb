<!DOCTYPE html>
<html>
  <head>
    <title>LayoutsAssignment</title>
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  <div class="container" style="margin-top:80px;">
    <div class="row">
      <div class="col-md-6">
        <%= yield :left_column %>
      </div>
      <div class="col-md-5">
        <%= yield :right_column %>
      </div>
    </div>

    <%= yield %>

  </div>
  </body>
</html>
